{
  "version": 2,
  "builds": [
    {
      "src": "Client/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "Server/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Server/dist/main.js",
      "methods":[ 
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/Client/$1"
    }
  ]
}