{
  "version": 2,
  "builds": [
    {
      "src": "Client/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "Server/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Server/dist/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/Client/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Client/build/$1"
    }
  ]
}